<h2><%= params[:status] ? params[:status].capitalize : 'All' %> Tickets</h2>
<table id="tickets" border="0" cellpadding="0" cellspacing="0">
  <% for ticket in @tickets %>
  <tr class="<%= cycle(['even', 'odd']) %>">
    <td><a href="/tickets/<%= ticket['id'] %>"><%= ticket['value'] %></a></td>
    <td><%= ticket['key'] %></td>
  </tr>
  <% end %>
</table>